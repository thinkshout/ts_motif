{#
/**
 * @file
 * Utility template for a field without any extra markup.
 *
 * Components can include this template for clean markup of the field items.
 * Example: {% include "@ts_motif/field/_field-clean.html.twig" %}
 */
#}
{% if items %}
  {% for item in items %}
    {{ item.content }}
  {% endfor %}
{% endif %}
